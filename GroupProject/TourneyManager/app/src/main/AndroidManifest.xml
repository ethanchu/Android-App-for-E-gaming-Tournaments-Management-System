<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.tourneymanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InactivemanagermodeActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PlayerMode"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PlayerModeUserlistActivity"
            android:label="@string/app_name"
            android:parentActivityName=".PlayerMode" />
        <activity
            android:name=".AddPlayerActivity"
            android:label="AddPlayer"
            android:parentActivityName=".InactivemanagermodeActivity" />
        <activity
            android:name=".HouseProfitsActivity"
            android:label="HouseProfits"
            android:parentActivityName=".InactivemanagermodeActivity" />
        <activity
            android:name=".UserlistActivity"
            android:label="Userlist"
            android:parentActivityName=".InactivemanagermodeActivity" />
        <activity
            android:name=".mgrReviewTournSetup"
            android:label="mgrReviewTournSetup"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".mgrSetupTournament"
            android:label="mgrSetupTournament"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".IndividualplayerActivity"
            android:label="mgrSetupTournament"
            android:parentActivityName=".UserlistActivity"></activity>
        <activity android:name=".mgrControlMatches" />
        <activity android:name=".mgrSelectMatchWinner" />
        <activity android:name=".ActiveManagerMode" />
    </application>

</manifest>